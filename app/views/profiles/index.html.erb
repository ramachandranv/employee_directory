<h1>My Profile Information</h1>

<table>
  <tr>
    <td>Name</td>
    <td><%= @profile.name %></td>
  </tr>
  <tr>
    <td>Address</td>
    <td><%= @profile.address %></td>
  </tr>
  <tr>
    <td>Phone</td>
    <td><%= @profile.phone %></td>
  </tr>
  <tr>
    <td>Email</td>
    <td><%= @profile.email %></td>
  </tr>
  <tr>
    <td><%= link_to "Show", @profile %></td>
    <td><%= link_to "Edit", edit_profile_path(@profile) %></td>
  </tr>
</table>

<br />

<%= link_to "Add Profile", new_profile_path %>
<%= link_to "Sign out", "/employees/sign_out" %>


